{
  "name": "sandbox",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "src/type.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "@originjs",
  "license": "MulanPSL2",
  "scripts": {
    "format": "prettier -w .",
    "lint": "eslint src/*/{src,types}/**",
    "lint:fix": "eslint --fix",
    "build": "rollup -c",
    "test": "jest --coverage --env ./src/__test__/test-env.js"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.2.3",
    "jsdom": "^17.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged --concurrent false",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {}
